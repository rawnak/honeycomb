%h{
#include <z-object.h>
%}

%{
#include <stdio.h>
%}

class ZTestObject : ZObject
{
	init(Self *self)
	{
	}

	public Self *new(struct zco_context_t *ctx)
	{
		Self *self = GET_NEW(ctx);
		return self;
	}

	public Self *clicked!(Self *self);

	public void click_it(Self *self)
	{
		clicked(self);
	}

	public void close1(Self *self)
	{
		fprintf(stderr, "close1 called!!\n");
	}

	public void close2(Self *self)
	{
		fprintf(stderr, "close2 called!!\n");
	}

}

